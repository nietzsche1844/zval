[1542 :unit/bnd-abs 1541 536870913] [1542 :unit/bnd-real 2 536870913] [1542 :unit/bnd-type :unit/bnd-comp 536870913] [1542 :unit/hash "unit:sha256:3c0541e17fc58ec665b1fb0656a394029be8100a24d1a269dc0ab86698081efc" 536870913] [1543 :unit/bnd-abs 1542 536870913] [1543 :unit/bnd-real 1540 536870913] [1543 :unit/bnd-type :unit/bnd-inst 536870913] [1543 :unit/hash "unit:sha256:90c4931fd9763881f22edf7a35aae2ed75f651d6ccbe790e32f1dcbdf25d07f3" 536870913] [1544 :atom/string "_Gegen die Nierenprüfer der Sittlichkeit._ — Man muss das Beste und das Schlechteste kennen, dessen ein Mensch fähig ist, im Vorstellen und Ausführen, um zu beurtheilen, wir stark seine sittliche Natur ist und wurde. Aber jenes zu erfahren ist unmöglich." 536870913] [1544 :unit/hash "unit:sha256:07387f33ad2a4848d9ffb361c50ef2ffbfcbcbd20129e3b0209870e40d72f478" 536870913] [1545 :unit/bnd-abs 1541 536870913] [1545 :unit/bnd-real 44 536870913] [1545 :unit/bnd-type :unit/bnd-comp 536870913] [1545 :unit/hash "unit:sha256:c7b66b69025cf7b5e5fe60c1ede1f04b918118af8048212a728c90088e225c17" 536870913] [1546 :unit/bnd-abs 1545 536870913] [1546 :unit/bnd-real 1544 536870913] [1546 :unit/bnd-type :unit/bnd-inst 536870913] [1546 :unit/hash "unit:sha256:799bf7415160dca8f415fc68bf76425d51c316dcefa31021050992cbc769ec05" 536870913] [1547 :atom/string "36." 536870913] [1547 :unit/hash "unit:sha256:3e63fd6eea743fa3d49a1f3728ee03f6d7e46500299aa37e7c6dfb6474c6e313" 536870913] [1548 :unit/bnd-abs 94 536870913] [1548 :unit/bnd-real 842 536870913] [1548 :unit/bnd-type :unit/bnd-comp 536870913] [1548 :unit/hash "unit:sha256:629eb8627e22e0c935e2688c036aec29da54db9f538464029e414ca74afbe41c" 536870913] [1549 :unit/bnd-abs 1548 536870913] [1549 :unit/bnd-real 2 536870913] [1549 :unit/bnd-type :unit/bnd-comp 536870913] [1549 :unit/hash "unit:sha256:d9c93515b910c10279a5f0600929d2d7ae5d47679dce0cf8ac57a004b7359ab0" 536870913] [1550 :unit/bnd-abs 1549 536870913] [1550 :unit/bnd-real 1547 536870913] [1550 :unit/bnd-type :unit/bnd-inst 536